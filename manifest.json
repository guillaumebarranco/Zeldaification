{
	"name": "Zeldaification",
	"version": "1.0",
	"description": "My chrome extension",
	"browser_action": {
	    "default_popup": "popup.html",
	    "default_icon": "img/favicon-160x160.png"
	},
	"icons": {
	    "160": "favicon-160x160"
	},
	"permissions": [
	    "tabs",
	    "storage",
	    "http://*/",
	    "https://*/"
	],
	"manifest_version": 2
}